{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "outDir": "./FRONTEND/dist",
        "rootDir": "./FRONTEND",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "strictNullChecks": true
    },
    "include": [
        "FRONTEND/assets/**/*.ts",
        "FRONTEND/interface/assets/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}